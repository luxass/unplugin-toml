{
  "name": "unplugin-toml",
  "version": "1.0.6",
  "description": "Allow importing TOML files as JSON or Strings",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@10.29.3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luxass/unplugin-toml.git"
  },
  "bugs": "https://github.com/luxass/unplugin-toml/issues",
  "keywords": [
    "toml",
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "esbuild",
    "nuxt",
    "astro"
  ],
  "exports": {
    ".": "./dist/index.mjs",
    "./astro": "./dist/astro.mjs",
    "./constants": "./dist/constants.mjs",
    "./esbuild": "./dist/esbuild.mjs",
    "./farm": "./dist/farm.mjs",
    "./nuxt": "./dist/nuxt.mjs",
    "./rolldown": "./dist/rolldown.mjs",
    "./rollup": "./dist/rollup.mjs",
    "./rspack": "./dist/rspack.mjs",
    "./types": {
      "types": "./toml.d.ts"
    },
    "./vite": "./dist/vite.mjs",
    "./webpack": "./dist/webpack.mjs",
    "./package.json": "./package.json"
  },
  "types": "dist/index.d.mts",
  "files": [
    "dist",
    "toml.d.ts"
  ],
  "engines": {
    "node": ">=20"
  },
  "workspaces": [
    "playground/*"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "eslint .",
    "test": "vitest --run",
    "test:watch": "vitest",
    "play:nuxt": "pnpm run --filter @unplugin-toml/nuxt-example dev",
    "play:vite-vue": "pnpm run --filter @unplugin-toml/vite-vue-example dev",
    "play:vite-react": "pnpm run --filter @unplugin-toml/vite-react-example dev",
    "play:nextjs": "pnpm run --filter @unplugin-toml/nextjs-example dev",
    "play:astro": "pnpm run --filter @unplugin-toml/astro-example dev",
    "play:rspack": "pnpm run --filter @unplugin-toml/rspack-example dev",
    "typecheck": "tsc --noEmit && pnpm -r typecheck"
  },
  "peerDependencies": {
    "@nuxt/kit": ">=3",
    "@nuxt/schema": ">=3",
    "@rspack/core": ">=1",
    "astro": ">=3",
    "esbuild": "*",
    "rolldown": ">=0.14.0",
    "rollup": ">=3",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "astro": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    },
    "@rspack/core": {
      "optional": true
    },
    "rolldown": {
      "optional": true
    }
  },
  "dependencies": {
    "@rollup/pluginutils": "catalog:prod",
    "smol-toml": "catalog:prod",
    "unplugin": "catalog:prod"
  },
  "devDependencies": {
    "@luxass/eslint-config": "catalog:dev",
    "@luxass/utils": "catalog:dev",
    "@nuxt/kit": "catalog:dev",
    "@nuxt/schema": "catalog:dev",
    "@rspack/core": "catalog:dev",
    "@types/node": "catalog:dev",
    "esbuild": "catalog:dev",
    "eslint": "catalog:dev",
    "publint": "catalog:dev",
    "rolldown": "catalog:dev",
    "rollup": "catalog:dev",
    "tsdown": "catalog:dev",
    "typescript": "catalog:dev",
    "vite": "catalog:dev",
    "vitest": "catalog:dev",
    "vitest-testdirs": "catalog:dev",
    "webpack": "catalog:dev"
  }
}
